            for(j=0;j<d;j++)
            {
                printf("L%dC%d= ",i,j);
                scanf("%d",&matB[i][j]);
                printf("\t");
            }

        }
    }
    for(int i=0;i<4;i++)
    {
        for(int j=0;j<4;j++)
        {
            D[i][j]=0;
